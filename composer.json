{
    "name": "apremiera/fixtures",
    "description": "Load fixtures into database from different formats: Yaml, XML, etc.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ApremierA",
            "email": "premier@list.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/yaml": "^5.0",
        "symfony/finder": "^5.0",
        "symfony/filesystem": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3 || ^6.0",
        "satooshi/php-coveralls": "dev-master"
